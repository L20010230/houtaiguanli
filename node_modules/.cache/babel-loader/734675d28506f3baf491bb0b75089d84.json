,CAAd,EAAiB,OAAOjB,QAAQ,CAACY,KAAD,EAAQV,MAAR,CAAf;AACjB,cAAI,CAACU,KAAL,EAAY,OAAO,EAAP;AACZG,UAAAA,OAAO,GAAGH,KAAV;AACH;AACJ;AAf4B,KAAjC;AAiBH,GAzBD;;AA0BAhB,EAAAA,WAAW,CAACE,OAAZ,CAAoBJ,GAApB,EAAyBC,GAAzB,EAA8BE,QAA9B;AACH,CA5BD","sourcesContent":["\nexports.ElementPlusUseLang = (app, ref, ElementPlus, zhLocale) => {\n    ElementPlus.useLang = (app, ref, locale) => {\n        const template = (str, option) => {\n            if (!str || !option) return str\n            return str.replace(/\\{(\\w+)\\}/g, (_, key) => {\n                return option[key]\n            })\n        }\n        // 注入全局属性,子组件都能通过inject获取\n        app.provide('ElLocaleInjection', {\n            lang: ref(locale.name),\n            locale: ref(locale),\n            t: (...args) => {\n                const [path, option] = args\n                let value\n                const array = path.split('.')\n                let current = locale\n                for (let i = 0, j = array.length; i < j; i++) {\n                    const property = array[i]\n                    value = current[property]\n                    if (i === j - 1) return template(value, option)\n                    if (!value) return ''\n                    current = value\n                }\n            },\n        })\n    }\n    ElementPlus.useLang(app, ref, zhLocale)\n}"]}]}